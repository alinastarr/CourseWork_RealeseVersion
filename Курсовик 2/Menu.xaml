<Window x:Class="Курсовик_2.Menu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Курсовик_2"
        mc:Ignorable="d"
        Title="Menu" Height="610" Width="1084" Background="#FF292929">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="90"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>



        </Grid.RowDefinitions>
        <Image Source="Resources/Mask Group.png" Margin="0,0,633,0" HorizontalAlignment="Left"></Image>
        <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="Меню" VerticalAlignment="Center" Foreground="White" FontSize="48"/>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" Grid.Row="1" Name="MainFrame"/>
        <Button x:Name="backButton" Content="Назад" HorizontalAlignment="Left" Margin="892,48,0,0" VerticalAlignment="Top" Width="133" Background="#FF373737" BorderBrush="#FF060606" Click="BackButton_Click" Height="32" Foreground="White" FontSize="18"/>
    </Grid>
</Window>
